generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int    @id @default(autoincrement()) // Primary Key, Auto Increment
  userId    String @unique(map: "user_id") @db.VarChar(50) // 사용자 ID (Unique)
  phoneNum  String @map("phone_num") @db.VarChar(15) // 전화번호
  age       Int @map("age")   // 나이

  @@map("users") // 테이블 이름을 "users"로 매핑
}



